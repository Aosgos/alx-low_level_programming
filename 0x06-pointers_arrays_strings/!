#include "main.h"

/**
 * *cap_string - main entry point to cap letters
 * @s: pointers pointing address to caps some ass
 * Return: 0, ./rest
 */

char *cap_string(char *s)
{
	int i = 0, len = 0;

	while (s[i++] != '\0')
	{
		if (s[i] >= 97 && s[i] <= 122)
			s[i] = s[i] + 32;
		len++;
	}
	return (s);
}
